
public class Greatest
 {
   public static void main(String args[])
   { 
    
   int a=20;
   int b=40;
   int c=40;
     if(a>b && a>c)
     
  {
    System.out.println(a+ " is greater ");
   }
    else if(b>c && b>a)
    {
    System.out.println(b+ " is greater");
    }
    else if(c>a && c>b)
    {
      System.out.println(c+" is greater");
    }
    else
    {System.out.print(a+"and"+b+"and"+c+"are equal");}
   }
 }
